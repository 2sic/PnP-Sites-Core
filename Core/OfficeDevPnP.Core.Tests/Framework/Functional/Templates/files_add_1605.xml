    </pnp:File>
    <pnp:File Src="garagewhite.spcolor" Folder="_catalogs/Theme/15" Overwrite="true" Level="Published"/>
    <pnp:File Src="fontscheme002.spfont" Folder="_catalogs/Theme/15" Overwrite="true" Level="Published"/>
    <pnp:Directory Src=".\DirFolder" Folder="DocLib1" Overwrite="true" Recursive="true" IncludedExtensions="*.docx,*.pdf" ExcludedExtensions="*.xml,*.txt" MetadataMappingFile=".\DirFolderMetadata.json" />
  <pnp:Directory Src=".\DirFolder2" Folder="DocLib2" Overwrite="true" Recursive="true" IncludedExtensions="*.docx,*.pdf" ExcludedExtensions="*.xml,*.txt" MetadataMappingFile=".\DirFolder2Metadata.json" />
